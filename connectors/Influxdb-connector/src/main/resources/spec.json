{
  "properties": {
    "name": "InfluxDB",
    "icon": "icons/influxDB.png",
    "id": "influxDB",
    "docs": "${docs}"
  },
  "configOptions": {
    "connection": {
      "type": "object",
      "properties": {
        "host": {
          "type": "string",
          "title": "${host}",
          "x-decorator": "FormItem",
          "x-component": "Input",
          "apiServerKey": "database_host",
          "required": true,
          "x-index": 10
        },
        "port": {
          "type": "string",
          "title": "${port}",
          "x-decorator": "FormItem",
          "x-component": "InputNumber",
          "apiServerKey": "database_port",
          "required": true,
          "x-index": 20
        },
        "maxinumLinesPerRequest": {
          "type": "string",
          "title": "${maxinumLinesPerRequest}",
          "x-decorator": "FormItem",
          "x-component": "Input",
          "required": true,
          "x-index": 30
        },
        "deserializer": {
          "type": "string",
          "title": "${deserializer}",
          "x-decorator": "FormItem",
          "x-component": "Input",
          "required": true,
          "x-index": 40
        },
        "username": {
          "type": "string",
          "title": "${username}",
          "x-decorator": "FormItem",
          "x-component": "Input",
          "apiServerKey": "database_user",
          "required": true,
          "x-index": 50
        },
        "password": {
          "type": "string",
          "title": "${password}",
          "x-decorator": "FormItem",
          "apiServerKey": "database_pwd",
          "x-component": "Input",
          "required": true,
          "x-index": 60
        },
        "precision": {
          "type": "string",
          "title": "${precision}",
          "x-decorator": "FormItem",
          "x-component": "Input",
          "apiServerKey": "database_timeformat",
          "required": true,
          "x-index": 70
        },
        "retension": {
          "type": "string",
          "title": "${retension}",
          "x-decorator": "FormItem",
          "x-component": "Input",
          "required": true,
          "x-index": 80
        },
        "timezone": {
          "type": "string",
          "title": "${timezone}",
          "default": "",
          "x-decorator": "FormItem",
          "x-decorator-props": {
            "feedbackLayout": "none"
          },
          "x-component": "Select",
